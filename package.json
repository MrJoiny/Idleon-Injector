{
    "name": "idleon-injector-ui",
    "version": "1.6.1",
    "description": "A cheat injector for Legends of Idleon",
    "main": "src/main.js",
    "bin": "src/main.js",
    "scripts": {
        "start": "node src/main.js",
        "validate": "node --check cheats.js config.js && node -e \"require('./src/ui/optionsAccountSchema.json')\"",
        "build": "pkg . -t node18-win-x64 --compress Gzip --output InjectCheatsUI.exe",
        "build-unix": "pkg . -t node18-linux-x64 --compress Gzip --output InjectCheatsUI"
    },
    "author": "MrJoiny",
    "dependencies": {
        "atob": "^2.1.2",
        "btoa": "^1.2.1",
        "chrome-remote-interface": "^0.33.0",
        "data-store": "^3.1.0",
        "enquirer": "^2.3.6",
        "express": "^4.18.2",
        "lodash": "^4.17.21",
        "prompt": "^1.3.0",
        "ws": "^8.13.0"
    },
    "pkg": {
        "assets": [
            "src/ui/**/*"
        ]
    }
}